{
  "comments": {
    "blockComment": ["(", ")"]
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" },
    { "open": "{", "close": "}" }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*To\\s+\"",
      "end": "^\\s*$"
    }
  },
  "wordPattern": "(-?\\d*\\.\\d+)|(-?\\d+)|([A-Za-z_][A-Za-z0-9_']*)"
}
