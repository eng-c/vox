{
  "name": "vox",
  "displayName": "Vox syntax highlighting",
  "description": "Syntax highlighting for Vox (sentence based code) (.vox files)",
  "version": "0.1.5",
  "publisher": "vox-lang",
  "engines": {
    "vscode": "^1.75.0"
  },
  "icon": "images/icon.png",
  "readme": "README.md",
  "files": [
    "README.md",
    "package.json",
    "language-configuration.json",
    "LICENSE",
    "syntaxes/",
    "images/"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vox",
        "aliases": ["Vox", "vox", "English", "english", "en"],
        "extensions": [".en", ".eng", ".vox", ".vx"],
        "icon": {
          "light": "./images/icon.png",
          "dark": "./images/icon.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vox",
        "scopeName": "source.vox",
        "path": "./syntaxes/vox.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "editor.tokenColorCustomizations": {
        "textMateRules": [
          {
            "scope": "entity.name.tag.each.vox",
            "settings": {
              "foreground": "#4EC9B0",
              "fontStyle": "bold"
            }
          },
          {
            "scope": "support.type.vox",
            "settings": {
              "foreground": "#FF79C6",
              "fontStyle": "bold"
            }
          },
          {
            "scope": "entity.name.function.definition.vox",
            "settings": {
              "foreground": "#DCDCAA",
              "fontStyle": "bold"
            }
          },
          {
            "scope": "entity.name.function.call.vox",
            "settings": {
              "foreground": "#DCDCAA"
            }
          },
          {
            "scope": "punctuation.definition.vox",
            "settings": {
              "foreground": "#56abd3"
            }
          },
          {
            "scope": "storage.type.vox",
            "settings": {
              "foreground": "#4EC9B0"
            }
          },
          {
            "scope": "keyword.control.vox",
            "settings": {
              "foreground": "#C586C0"
            }
          },
          {
            "scope": "keyword.other.vox",
            "settings": {
              "foreground": "#C586C0"
            }
          },
          {
            "scope": "keyword.operator.vox",
            "settings": {
              "foreground": "#ebe6be"
            }
          },
          {
            "scope": "constant.language.boolean.vox",
            "settings": {
              "foreground": "#569CD6"
            }
          },
          {
            "scope": "support.function.vox",
            "settings": {
              "foreground": "#b0dcaa"
            }
          },
          {
            "scope": "variable.other.interpolation.vox",
            "settings": {
              "foreground": "#F1FA8C",
              "fontStyle": "bold"
            }
          }
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vox-lang/vox"
  },
  "license": "MIT"
}
